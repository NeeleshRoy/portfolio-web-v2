{
	"name": "portfolio-web-v2",
	"version": "0.0.1",
	"description": "Render application for portfolio of Neelesh Roy",
	"main": "index.jsx",
	"scripts": {
		"build": "npm run clean && rollup -c --environment INCLUDE_DEPS,BUILD:production",
		"build:watch": "npm run clean && rollup -c -m --watch --environment INCLUDE_DEPS,BUILD:development",
		"clean": "rm -rf dist",
		"start": "npm run build:watch",
		"healthcheck": "npm run lint",
		"lint": "npm run lint:js && npm run lint:css",
		"lint:js": "eslint src",
		"lint:css": "stylelint src/sass/**/*.scss",
		"lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NeeleshRoy/portfolio-web-v2.git"
	},
	"author": "Neelesh Roy",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/NeeleshRoy/portfolio-web-v2/issues"
	},
	"homepage": "https://github.com/NeeleshRoy/portfolio-web-v2#readme",
	"dependencies": {
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.14.8",
		"@babel/plugin-proposal-object-rest-spread": "^7.14.7",
		"@babel/preset-env": "^7.14.9",
		"@babel/preset-react": "^7.14.5",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-replace": "^3.0.0",
		"cssnano": "^4.1.11",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"node-sass": "^6.0.1",
		"postcss-cssnext": "^3.1.0",
		"postcss-nested": "^5.0.6",
		"postcss-simple-vars": "^6.0.3",
		"rollup": "^2.55.1",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-postcss": "^4.0.0",
		"rollup-plugin-sass": "^1.2.2",
		"rollup-plugin-serve": "^1.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-watch": "^4.3.1",
		"stylelint": "^13.13.1",
		"stylelint-config-sass-guidelines": "^8.0.0"
	}
}

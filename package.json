{
  "name": "portfolio-web-v2",
  "version": "0.0.1",
  "description": "Render application for portfolio of Neelesh Roy",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && rollup -c --environment INCLUDE_DEPS,BUILD:production",
